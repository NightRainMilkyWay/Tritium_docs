{
  "private": "true",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vitepress build --base /RedemptionProject/",
    "build-mpa": "vitepress build --mpa --base /RedemptionProject/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "dev": "vitepress --base /RedemptionProject/ --port 1999",
    "postinstall": "husky install",
    "lint": "prettier  --write . && eslint --fix --ext .js,.ts,.vue",
    "lint:check": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:zh",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "markdownlint **/*.md && lint:zh",
    "lint:prettier": "prettier --check .",
    "lint:zh": "zhlint 'docs/**/*.md'",
    "serve": "vitepress serve --base /RedemptionProject/",
    "site:preview": "vitepress build --outDir _dist"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "prettier": "prettier-config-vuepress",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@iconify-json/ic": "^1.1.14",
    "@iconify-json/logos": "^1.1.38",
    "@iconify-json/mdi": "^1.1.55",
    "@iconify-json/ph": "^1.1.6",
    "@mdit/plugin-container": "^0.6.5",
    "@mdit/plugin-mark": "^0.6.5",
    "@types/js-yaml": "^4.0.9",
    "@vueuse/core": "^10.6.1",
    "@vueuse/integrations": "^10.6.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "feed": "^4.2.2",
    "js-yaml": "^4.1.0",
    "markdown-it-footnote": "^3.0.3",
    "medium-zoom": "^1.1.0",
    "qrcode": "^1.5.3",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "unocss": "^0.57.5",
    "vitepress": "^1.0.0-rc.27",
    "vitepress-markdown-timeline": "^1.2.1",
    "vue-amazing-ui": "^0.1.19",
    "vue-i18n": "^9.7.0",
    "vue-router": "^4.2.5",
    "vue-router-better-scroller": "^0.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@commitlint/types": "^18.4.0",
    "@mdit/plugin-figure": "^0.6.5",
    "@mdit/plugin-img-lazyload": "^0.6.5",
    "@mdit/plugin-img-mark": "^0.6.5",
    "@mdit/plugin-img-size": "^0.6.5",
    "@mdit/plugin-sub": "^0.6.5",
    "@mdit/plugin-sup": "^0.6.5",
    "@types/eslint": "^8.44.7",
    "@types/markdown-it": "^13.0.6",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@unocss/eslint-config": "^0.57.5",
    "@vite-pwa/vitepress": "^0.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-vuepress": "^4.5.0",
    "eslint-config-vuepress-typescript": "^4.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "9.18.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.1.0",
    "markdown-it": "^13.0.2",
    "markdownlint-cli": "^0.37.0",
    "pnpm": "^8.10.5",
    "prettier": "^3.1.0",
    "prettier-config-vuepress": "^4.4.0",
    "sort-package-json": "^2.6.0",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "tsconfig-vuepress": "^4.5.0",
    "vite-plugin-inspect": "^0.7.42",
    "vue": "^3.3.8",
    "zhlint": "^0.7.1"
  },
  "packageManager": "pnpm@8.6.12",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  }
}
