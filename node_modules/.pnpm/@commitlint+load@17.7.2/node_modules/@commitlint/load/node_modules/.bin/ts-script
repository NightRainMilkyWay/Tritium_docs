#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ts-node@10.9.1_@types+node@20.8.7_typescript@5.2.2/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../../../../../../ts-node@10.9.1_@types+node@20.8.7_typescript@5.2.2/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
fi
